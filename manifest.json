{
    "manifest_version": 3,
    "name": "Kuika Helper",
    "version": "1.0",
    "description": "Kuika Helper",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.kuika.com/*",
                "http://localhost:3000/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}